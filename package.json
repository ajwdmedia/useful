{
	"name": "@ajwdmedia/useful",
	"version": "0.3.4",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"import": "./dist/index.js"
		},
		"./copy": {
			"types": "./dist/copy/index.d.ts",
			"svelte": "./dist/copy/index.js",
			"import": "./dist/copy/index.js"
		},
		"./formatters": {
			"types": "./dist/formatters/index.d.ts",
			"svelte": "./dist/formatters/index.js",
			"import": "./dist/formatters/index.js"
		},
		"./stores": {
			"types": "./dist/stores/index.d.ts",
			"svelte": "./dist/stores/index.js",
			"import": "./dist/stores/index.js"
		}
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"svelte": "^4.1.2"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.22.4",
		"@sveltejs/package": "^2.2.0",
		"publint": "^0.2.0",
		"sass": "^1.64.2",
		"svelte": "^4.1.2",
		"svelte-check": "^3.4.6",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.0.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
